fn main(){
    let rect = Rectangle {
        length: 35,
        breadth: 20,
        width: 5,
    };
    println!("The area of the rectangle is {}", rect.area());
}

struct Rectangle {
    length : u16,
    breadth : u16,
    width : u16,
}

impl Rectangle{
    fn area(&self) -> u16 {
        self.length * self.breadth
    }
}
